{
    "name": "nswdpc/silverstripe-search-boilerplate",
    "description": "Search boilerplate for NSWDPC Silverstripe websites, using elasticsearch configuration",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "James Ellis",
            "homepage": "https://dpc.nsw.gov.au",
            "role": "Developer"
        },
        {
            "name": "Mark Taylor",
            "homepage": "https://dpc.nsw.gov.au",
            "role": "Developer"
        }
    ],
    "keywords": [
      "silverstripe",
      "search",
      "elastic",
      "elastica",
      "elasticsearch"
    ],
    "require": {
        "heyday/silverstripe-elastica": "^2.2|^4.1",
        "nglasl/silverstripe-extensible-search": "dev-feat-silverstripe-5",
        "nyeholt/silverstripe-extensible-elastic": "dev-feat-silverstripe-5",
        "nswdpc/silverstripe-elemental-extensible-search": "dev-feat-silverstripe-5",
        "symbiote/silverstripe-multivaluefield": "^6"
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
